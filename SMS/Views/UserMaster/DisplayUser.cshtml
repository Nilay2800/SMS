@model IEnumerable<SMS.Model.Signups>

@{
    ViewBag.Title = "DisplayUser";
}

<h2>DisplayUser</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConfirmPassword)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Role)
            </th>*@
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.Password)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ConfirmPassword)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Role)
                </td>*@
            <td>
                @Html.ActionLink("Edit", "EditUserRoleMapping", new { id = item.Userid }, htmlAttributes: new { @class = "mybtn" }) |
                @Html.ActionLink("Delete", "DeleteUser", new { id = item.Userid }, htmlAttributes: new { @class = "mybtn" })
            </td>
        </tr>
        <style type="text/css">
            a.mybtn {
                background: #777;
                color: #fff;
                display: inline-block;
                padding: 6px 12px 6px 12px;
                text-decoration: none;
            }
        </style>
    }

</table>
